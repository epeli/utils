{
    "exclude": ["type-checks"],
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noEmit": true,
        "lib": ["es6", "dom"],
        "moduleResolution": "node",
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "esModuleInterop": true
    }
}
